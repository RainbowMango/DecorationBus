platform :ios, '8.0'
use_frameworks!

#There are no targets called "Shows" in any Xcode projects
#If you want multiple targets to share the same pods, use an abstract_target.
abstract_target 'MyApp' do

    target 'DecorationBus' do
        pod 'MWPhotoBrowser',          '~> 2.1.1'
        pod 'UMengAnalytics-NO-IDFA',  '~> 3.4.7'
        pod 'MJRefresh',               '~> 3.0.4'
        pod 'MDRadialProgress',        '~> 1.3.2'
        pod 'MOBFoundation_IDFA',      '~> 1.5.1'
        pod 'SMSSDK',                  '~> 2.0.4'
        pod 'KMPlaceholderTextView',   '~> 1.1.2'
        pod 'InfiniteCollectionView',  '~> 1.0.2'
        pod 'StarReview',              '~> 0.0.2'
        pod 'SKPhotoBrowser',          '~> 1.7.6'
        pod 'Alamofire',               '~> 3.3.0'
        pod 'DKImagePickerController', '~> 3.1.3'
    end

    target 'DecorationBus Dev' do
        pod 'MWPhotoBrowser',          '~> 2.1.1'
        pod 'UMengAnalytics-NO-IDFA',  '~> 3.4.7'
        pod 'MJRefresh',               '~> 3.0.4'
        pod 'MDRadialProgress',        '~> 1.3.2'
        pod 'MOBFoundation_IDFA',      '~> 1.5.1'
        pod 'SMSSDK',                  '~> 2.0.4'
        pod 'KMPlaceholderTextView',   '~> 1.1.2'
        pod 'InfiniteCollectionView',  '~> 1.0.2'
        pod 'StarReview',              '~> 0.0.2'
        pod 'SKPhotoBrowser',          '~> 1.7.6'
        pod 'Alamofire',               '~> 3.3.0'
        pod 'DKImagePickerController', '~> 3.1.3'
    end

end
